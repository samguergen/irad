<%= form_for @artist do |f| %>
  <%= f.label 'stage name'%>:
  <%= f.text_field :stage_name%><br />
  <%= f.label 'birth name (if different)'%>:
  <%= f.text_field :birth_name%><br />
  <%= f.label 'age'%>:
  <%= f.text_field :age%><br />
  <%#= f.label 'record label'%>
  <!-- <%#= f.text_field 'record_label' %><br /> -->
  <%= f.label 'description'%>:
  <%= f.text_field :description %><br />
  <%= f.submit %>
<% end %>