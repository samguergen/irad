<%= form_for @album, url: {action: "create"} do |f| %>
  <%= render 'shared/error_messages', target: @album %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label 'year released' %>
  <%= f.text_field :release_year %>
  <%= f.label 'description' %>
  <%= f.text_field :description %>
  <%= f.submit %>
<% end %>