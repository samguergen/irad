<%= form_for @album, url: {action: "update"} do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label 'release year' %>
  <%= f.text_field :release_year %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.submit %>
<% end %>

<%= link_to 'Delete Album', artist_album_path, method: :delete %>